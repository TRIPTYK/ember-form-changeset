{
  "name": "ember-form-changeset-validations",
  "version": "1.3.0",
  "description": "Helps creating form components logic based on ember-changeset-validations.",
  "keywords": [
    "ember-addon",
    "changeset",
    "generator",
    "form"
  ],
  "repository": "https://github.com/TRIPTYK/ember-form-changeset-validations",
  "license": "MIT",
  "author": "TRIPTYK",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "_test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "rm -rf utils && rm -rf components && rm index.d.ts"
  },
