{
  "name": "ember-immer-changeset-root",
  "version": "1.0.2",
  "private": true,
  "repository": "https://github.com/TRIPTYK/ember-immer-changeset",
  "license": "MIT",
  "author": "AmauryD",
  "keywords": [
    "ember-addon",
    "changeset",
    "immer",
    "validation",
    "typescript"
  ],
  "scripts": {
    "build": "pnpm --filter ember-immer-changeset build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-immer-changeset start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "volta": {
    "node": "20.18.0",
    "pnpm": "9.12.1"
  }
}
