{
  "name": "ember-form-changeset-validations",
  "version": "2.3.5",
  "description": "A changeset library for emberJS.",
  "keywords": [
    "ember-addon",
    "changeset",
    "typescript"
  ],
  "repository": "https://github.com/TRIPTYK/ember-form-changeset-validations",
  "license": "MIT",
  "author": "TRIPTYK",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "_test:ember-compatibility": "ember try:each",
    "build": "rollup --config",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "postpack": "rm -rf utils && rm -rf components && rm index.d.ts",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "test:ember": "ember test"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.5",
    "@glimmer/component": "~1.1.2",
    "@glimmer/tracking": "1.1.2",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-typescript": "^5.2.1",
    "immer": "~10.0.2",
    "ember-concurrency-ts": "~0.3.1",
    "@ember/test-waiters": "~3.0.2",
    "ember-concurrency":"~3.0.0",
    "short-uuid": "~4.2.2"
  },
  "devDependencies": {
    "@types/ember__object": "~4.0.6",
    "@types/ember__owner": "~4.0.4",
    "@types/ember__debug": "~4.0.4",
    "typescript": "~5.1.6",
    "type-fest": "~3.12.0",
    "@tsconfig/ember":"~3.0.0",
    "ember-source": "~4.12.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@embroider/addon-dev": "^3.1.1",
    "rollup": "^3.24.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ts": "^3.2.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./changeset/immer-changeset.js": "./dist/_app_/changeset/immer-changeset.js",
      "./components/immer-form.js": "./dist/_app_/components/immer-form.js",
      "./index.js": "./dist/_app_/index.js",
      "./types/typed-changeset.js": "./dist/_app_/types/typed-changeset.js",
      "./utils/is-changeset-array.js": "./dist/_app_/utils/is-changeset-array.js",
      "./utils/is-changeset.js": "./dist/_app_/utils/is-changeset.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "volta": {
    "node": "16.19.1"
  },
  "packageManager": "pnpm@7.28.0",
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}
